PROJ_TMPDIR = $(BLD_TMPDIR)/$(PROJECT)/$(PACKAGE)
PROJ_CLEAN += $(PROJ_BIN)

PROJ_CXX_INCLUDES += $(BUILD_INCDIR)

PROJ_CXX_INCLUDES += ${CPPUNIT_INCDIR}
PROJ_LCXX_FLAGS += -L$(CPPUNIT_LIBDIR) -lcppunit -ldl

PROJ_LCXX_FLAGS += -L$(PYTHIA_LIBDIR) -ljournal
PROJ_LCXX_FLAGS += -L$(PYTHIAUTIL_LIBDIR) -lpythiautil
PROJ_LCXX_FLAGS += -lspatialdata
PROJ_LCXX_FLAGS += -L$(PYTHON_LIBDIR) -lpython$(PYTHON_VERSION)

PROJ_LCXX_FLAGS += $(EXTERNAL_LIBS)

