#if !defined(_spatial_testsimpledbvolume_h)
#error "TestSimpleDBVolume.icc must only be included from TestSimpleDBVolume.h"
#endif

// Get data for database.
inline
const double*
spatial::TestSimpleDBVolume::Data(void) const
{ return DATA; }

// Get names of values in database.
inline
const char**
spatial::TestSimpleDBVolume::Names(void) const
{ return NAMES; }

// Get number of locations in database.
inline
int 
spatial::TestSimpleDBVolume::NumLocs(void) const
{ return NUMLOCS; }

// Get number of values at each location in database.
inline
int
spatial::TestSimpleDBVolume::NumVals(void) const
{ return NUMVALS; }

// Get topology of data in database.
inline
spatial::SimpleDB::TopoEnum
spatial::TestSimpleDBVolume::Topology(void) const
{ return TOPOLOGY; }

// Get number of queries.
inline
int
spatial::TestSimpleDBVolume::NumQueries(void) const
{ return NUMQUERIES; }

// Get query data for nearest neighbor algorithm.
inline
const double*
spatial::TestSimpleDBVolume::QueryNearest(void) const
{ return QUERYNEAREST; }

// Get query data for linear interpolation algorithm.
inline
const double*
spatial::TestSimpleDBVolume::QueryLinear(void) const
{ return QUERYLINEAR; }

